					<ProtectedString name="Source"><![CDATA[-- File: ServerStorage/Modules/AbilityModule.lua

local ServerStorage = script.Parent.Parent
local AbilityData = require(ServerStorage.Data.AbilityData)
local MoveData = require(ServerStorage.Data.MoveData)

local AbilityModule = {}
AbilityModule.ABILITY_DATA = AbilityData.ABILITY_DATA

-- Helper function that executes a list of effects on a target
-- RETURNS: table of structured combat log events.
local function executeEffects(abilityName: string, userCreature: table, targetCreature: table, effectList: table): table
	local events = {} 

	for _, effectAction in ipairs(effectList) do
		local actionType = effectAction[1]

		if actionType == "StatChange" then
			local statName = effectAction[2]
			local stages = effectAction[3]

			targetCreature:ApplyStatBoost(statName, stages) -- PSEUDO-CODE: Execute logic

			table.insert(events, {
				EventType = "StatChange", Source = userCreature.ID, Target = targetCreature.ID,
				Ability = abilityName, Stat = statName, Stages = stages, Success = true
			})

		elseif actionType == "RecoilDamage" or actionType == "RecoilDamageFixed" then
			local multiplier = effectAction[2]

			table.insert(events, {
				EventType = "DamageIntent", Source = targetCreature.ID, Target = userCreature.ID,
				Ability = abilityName, DamageType = "Recoil", RecoilMultiplier = multiplier,
				Success = true
			})

		elseif actionType == "HealFractionMaxHP" then
			local fraction = effectAction[2]
			local healingAmount = targetCreature.MaxHP * fraction 
			targetCreature:Heal(healingAmount) -- PSEUDO-CODE

			table.insert(events, {
				EventType = "Healing", Source = userCreature.ID, Target = targetCreature.ID,
				Ability = abilityName, Amount = healingAmount
			})

		elseif actionType == "GiveStatus" then
			local status = effectAction[2]
			local chance = effectAction[3]

			local success = targetCreature:AttemptApplyStatusEffect(status, chance) -- PSEUDO-CODE

			table.insert(events, {
				EventType = "StatusAttempt", Source = userCreature.ID, Target = targetCreature.ID,
				Ability = abilityName, Status = status, Chance = chance, Success = success,
			})
		end
	end
	return events
end

-- --- PUBLIC HANDLERS ---

function AbilityModule.getAbilityData(abilityName: string): table
	return AbilityModule.ABILITY_DATA[abilityName]
end

-- 1. On Entry Handler (e.g., Intimidate, Dragon Surge)
-- RETURNS: table of events (ability activation, stat changes).
function AbilityModule.handleOnEntry(user: table, allOpponents: table): table
	local collectedEvents = {}
	local abilityName = user.ActiveAbility or user.HiddenAbility
	if not abilityName then return collectedEvents end

	local abilityData = AbilityModule.getAbilityData(abilityName)

	if abilityData and table.find(abilityData.Trigger, "OnEntry") then
		local abilityDisplayName = abilityData.Name
		local scope = abilityData.Scope

		table.insert(collectedEvents, {
			EventType = "AbilityActivate", Source = user.ID, Ability = abilityDisplayName,
			Message = string.format("%s's %s activated!", user.Name, abilityDisplayName) 
		})

		if scope == "UserOnly" then
			local effectEvents = executeEffects(abilityDisplayName, user, user, abilityData.Effect)
			for _, event in ipairs(effectEvents) do table.insert(collectedEvents, event) end
		elseif scope == "OpponentAll" then
			for _, opponent in ipairs(allOpponents) do
				local effectEvents = executeEffects(abilityDisplayName, user, opponent, abilityData.Effect)
				for _, event in ipairs(effectEvents) do table.insert(collectedEvents, event) end
			end
		end
	end
	return collectedEvents
end

-- 2. Pre-Damage Calculation Handler (e.g., Pyre Power, Overgrow)
-- RETURNS: number (damage multiplier).
function AbilityModule.calculateDamageMultiplier(user: table, move: table): number
	local multiplier = 1.0
	local abilityName = user.ActiveAbility
	if not abilityName then return multiplier end

	local abilityData = AbilityModule.getAbilityData(abilityName)

	-- Passive Type Boosts (e.g., Pyre Power)
	if abilityData and table.find(abilityData.Trigger, "OnAttackExecuted") then
		for _, effectAction in ipairs(abilityData.Effect) do
			if effectAction[1] == "BoostTypeDamage" and effectAction[2] == move.Type then
				multiplier = multiplier * effectAction[3]
			end
		end
	end

	-- Conditional HP Boosts (e.g., Overgrow, Torrent)
	if abilityData and table.find(abilityData.Trigger, "HealthLow") then
		local userHPPercent = user.CurrentHP / user.MaxHP -- PSEUDO-CODE

		if userHPPercent <= 0.33 then
			for _, effectAction in ipairs(abilityData.Effect) do
				if effectAction[1] == "ConditionalBoostTypeDamage" and effectAction[2] == move.Type then
					multiplier = multiplier * effectAction[3]
				end
			end
		end
	end

	return multiplier
end

-- 3. Post-Damage Handler (e.g., Molten Scales, Poison Touch)
-- RETURNS: table of events (recoil, status attempts).
function AbilityModule.handlePostDamageEffects(attacker: table, victim: table, move: table, isContact: boolean): table
	local collectedEvents = {}
	local victimAbility = victim.ActiveAbility or victim.HiddenAbility
	local attackerAbility = attacker.ActiveAbility or attacker.HiddenAbility 

	-- Victim's damage taken effects (Recoil, Status on hit)
	if victimAbility then
		local vAbilityData = AbilityModule.getAbilityData(victimAbility)
		local vAbilityName = vAbilityData.Name

		if (move.Category == "Physical" and table.find(vAbilityData.Trigger, "OnPhysicalDamageTaken")) or
			(isContact and table.find(vAbilityData.Trigger, "OnContactDamageTaken")) then

			table.insert(collectedEvents, {EventType = "AbilityActivate", Source = victim.ID, Ability = vAbilityName})
			local effectEvents = executeEffects(vAbilityName, victim, attacker, vAbilityData.Effect)
			for _, event in ipairs(effectEvents) do table.insert(collectedEvents, event) end
		end
	end

	-- Attacker's contact effects (Status on hit)
	if attackerAbility and isContact then
		local aAbilityData = AbilityModule.getAbilityData(attackerAbility)
		local aAbilityName = aAbilityData.Name

		if table.find(aAbilityData.Trigger, "OnContactHit") then
			table.insert(collectedEvents, {EventType = "AbilityActivate", Source = attacker.ID, Ability = aAbilityName})
			local effectEvents = executeEffects(aAbilityName, attacker, victim, aAbilityData.Effect)
			for _, event in ipairs(effectEvents) do table.insert(collectedEvents, event) end
		end
	end
	return collectedEvents
end


-- 4. Global Move Used Handler (e.g., Wyrm Resonance)
-- RETURNS: table of events (stat boosts).
function AbilityModule.handleOnMoveUsed(moveUser: table, moveName: string, activeCreaturesInBattle): table
	local collectedEvents = {}

	for _, creature in ipairs(activeCreaturesInBattle) do
		local abilityName = creature.ActiveAbility or creature.HiddenAbility
		if not abilityName then continue end

		local abilityData = AbilityModule.getAbilityData(abilityName)

		if abilityData and table.find(abilityData.Trigger, "OnMoveUsedGlobal") then
			local conditionMove = abilityData.Trigger[2]

			if conditionMove and moveName == conditionMove then
				table.insert(collectedEvents, {EventType = "AbilityActivate", Source = creature.ID, Ability = abilityData.Name})

				local effectEvents = executeEffects(abilityData.Name, creature, creature, abilityData.Effect)
				for _, event in ipairs(effectEvents) do table.insert(collectedEvents, event) end
			end
		end
	end
	return collectedEvents
end

--- Interception and Future Handlers

-- 5. Status Immunity Check (e.g., Immunity, Water Veil)
-- Called BEFORE applying status. RETURNS: boolean (true if immune).
function AbilityModule.checkStatusImmunity(target: table, statusType: string): boolean
	local abilityName = target.ActiveAbility or target.HiddenAbility
	if not abilityName then return false end 
	-- FUTURE LOGIC: Check AbilityData for statuses the target is immune to.
	return false 
end

-- 6. End-of-Turn Handler (e.g., Passive Healing)
-- Called at the end of every turn. RETURNS: table of events (healing, damage).
function AbilityModule.handleEndOfTurn(creature: table): table
	local collectedEvents = {}
	local abilityName = creature.ActiveAbility or creature.HiddenAbility
	if not abilityName then return collectedEvents end

	local abilityData = AbilityModule.getAbilityData(abilityName)

	if abilityData and table.find(abilityData.Trigger, "EndOfTurn") then
		local abilityDisplayName = abilityData.Name

		table.insert(collectedEvents, {EventType = "AbilityActivate", Source = creature.ID, Ability = abilityDisplayName})

		local effectEvents = executeEffects(abilityDisplayName, creature, creature, abilityData.Effect)
		for _, event in ipairs(effectEvents) do table.insert(collectedEvents, event) end
	end
	return collectedEvents
end

-- 7. Damage Prevention Handler (e.g., Rock Head)
-- Called BEFORE applying self-inflicted damage (recoil, crash). RETURNS: boolean (true if damage is prevented).
function AbilityModule.checkDamagePrevention(user: table, damageType: string): boolean
	local abilityName = user.ActiveAbility or user.HiddenAbility
	if not abilityName then return false end

	local abilityData = AbilityModule.getAbilityData(abilityName)

	if abilityData and table.find(abilityData.Trigger, "OnSelfDamage") then
		for _, effectAction in ipairs(abilityData.Effect) do
			-- Checks for structure: ["PreventDamageType", "Recoil"]
			if effectAction[1] == "PreventDamageType" and effectAction[2] == damageType then
				return true -- Prevention successful
			end
		end
	end
	return false
end

-- 8. Stat Change Interception Handler (e.g., Clear Body, Inner Focus)
-- Called by the target's instance BEFORE applying a stat change. RETURNS: boolean (true if stat change is prevented).
function AbilityModule.checkStatChangeInterception(target: table, statName: string, stages: number, sourceIsOpponent: boolean): boolean
	local abilityName = target.ActiveAbility or target.HiddenAbility
	if not abilityName then return false end

	-- Only check for negation if the stat is dropping and the source is external
	if stages >= 0 or not sourceIsOpponent then 
		return false 
	end

	local abilityData = AbilityModule.getAbilityData(abilityName)

	if abilityData and table.find(abilityData.Trigger, "OnOpponentStatChangeAttempt") then
		for _, effectAction in ipairs(abilityData.Effect) do
			-- Checks for structure: ["PreventOpponentStatDrop", "Attack"] or ["PreventOpponentStatDrop", "All"]
			local actionType = effectAction[1]
			local targetStat = effectAction[2]

			if actionType == "PreventOpponentStatDrop" then
				-- Check if the ability prevents this specific stat drop (e.g., "All" or "Attack")
				if targetStat == "All" or targetStat == statName then
					return true -- Prevention successful, stat change is negated
				end
			end
		end
	end
	return false 
end


--- Passive Modification Handlers (Numeric Returns)

-- 9. Type Defense Modification Handler (e.g., Thick Fat)
-- RETURNS: number (multiplies type effectiveness).
function AbilityModule.getTypeDefenseMultiplier(defender: table, moveType: string): number
	local multiplier = 1.0
	local abilityName = defender.ActiveAbility or defender.HiddenAbility
	if not abilityName then return multiplier end

	local abilityData = AbilityModule.getAbilityData(abilityName)

	if abilityData and table.find(abilityData.Trigger, "OnDamageReceived") then
		for _, effectAction in ipairs(abilityData.Effect) do
			if effectAction[1] == "TypeDefenseModify" and effectAction[2] == moveType then
				multiplier = multiplier * effectAction[3]
			end
		end
	end
	return multiplier
end

-- 10. Environment/Weather Stat Check Handler (e.g., Swift Swim)
-- RETURNS: number (multiplies stat value, e.g., Speed * 2.0).
function AbilityModule.getStatCheckMultiplier(user: table, statName: string, environmentCondition: string): number
	local multiplier = 1.0
	local abilityName = user.ActiveAbility or user.HiddenAbility
	if not abilityName then return multiplier end

	local abilityData = AbilityModule.getAbilityData(abilityName)

	if abilityData and table.find(abilityData.Trigger, "OnWeatherChange") then
		for _, effectAction in ipairs(abilityData.Effect) do
			local action, requiredCondition, requiredStat, boost = unpack(effectAction)

			if action == "ConditionStatBoost" and requiredStat == statName and requiredCondition == environmentCondition then
				multiplier = multiplier * boost
			end
		end
	end
	return multiplier
end

return AbilityModule]]></ProtectedString>
