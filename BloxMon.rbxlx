					<ProtectedString name="Source"><![CDATA[-- CreatureModule (In ServerStorage)
-- Calculates their actual stats based on level.
local CreatureData = require(script.Parent.Parent.Data.CreatureData)
local CreatureModule = {}
CreatureModule.BASE_DATA = CreatureData.BASE_DATA

-- [[ 1. The Stat Calculation Function ]]
function CreatureModule:CalculateStats(baseStats: table, level: number): table
	local calculatedStats = {}
	calculatedStats.HP = math.floor( (2 * baseStats.HP * level) / 100 + level + 10 )
	for statName, baseValue in pairs(baseStats) do
		if statName ~= "HP" then
			calculatedStats[statName] = math.floor( (2 * baseValue * level) / 100 + 5 )
		end
	end
	return calculatedStats
end

-- [[ 2. Helper Function: Get Learnable Moves (Updated with Full Evolution Check) ]]
function CreatureModule.getMovesLearnedAtLevel(creatureID: number, level: number): table
	local learnedMoves = {} -- { [moveName] = true, ... } for uniqueness
	local moveOrderMap = {}

	local BASE_DATA = CreatureModule.BASE_DATA 

	local function checkEvolutionChain(currentID: number, currentLevelLimit: number)
		local currentData = BASE_DATA[currentID]
		if not currentData then return end

		for _, data in pairs(BASE_DATA) do
			if data.Evolutions and data.Evolutions.TargetID == currentID then
				checkEvolutionChain(data.ID, data.Evolutions.Condition)
				break
			end
		end

		if currentData.BaseMoves then
			for _, moveName in ipairs(currentData.BaseMoves) do
				if not learnedMoves[moveName] then
					learnedMoves[moveName] = true
					moveOrderMap[moveName] = currentLevelLimit 
				end
			end
		end

		local maxLevelCheck = (currentID == creatureID) and currentLevelLimit or currentData.Evolutions.Condition

		for requiredLevel, moves in pairs(currentData.LevelUpMoves) do
			if requiredLevel <= maxLevelCheck then
				for _, moveName in ipairs(moves) do
					if not learnedMoves[moveName] then
						learnedMoves[moveName] = true
						moveOrderMap[moveName] = requiredLevel
					end
				end
			end
		end
	end

	-- Start the recursive check from the final creature's ID and level
	checkEvolutionChain(creatureID, level)

	-- Convert the moves into a final list, ordered by the level they were learned
	local orderedMoves = {}
	local sortedMovesByLevel = {}

	-- Populate a table with {level, moveName} pairs
	for moveName, learnLevel in pairs(moveOrderMap) do
		table.insert(sortedMovesByLevel, {Level = learnLevel, Name = moveName})
	end

	-- Sort the moves based on their learning level (ascending)
	table.sort(sortedMovesByLevel, function(a, b)
		-- Primary sort key: Level
		if a.Level ~= b.Level then
			return a.Level < b.Level
		end
		-- Secondary sort key (for moves learned at the same level): Alphabetical name
		return a.Name < b.Name
	end)

	-- Extract just the move names into the final ordered list
	for _, moveData in ipairs(sortedMovesByLevel) do
		table.insert(orderedMoves, moveData.Name)
	end

	return orderedMoves
end

-- [[ 3. The Constructor Function (The "Instance" Creator) ]]

local CreatureInstance = {}
CreatureInstance.__index = CreatureInstance
--- Creates a new, active creature object (an instance) for use in battle or player inventory.
function CreatureModule.new(creatureID: number, level: number): table
	local baseData = CreatureModule.BASE_DATA[creatureID]
	if not baseData then
		warn("Creature ID", creatureID, "not found!")
		return {}
	end

	local self = setmetatable({}, CreatureInstance)
	-- Static properties inherited from the blueprint
	self.ID = creatureID
	self.Name = baseData.Name
	self.Type = baseData.Type

	-- Note: Renaming baseData.Ability to baseData.BaseAbilities is recommended
	local possibleAbilities = baseData.Ability -- Array of standard abilities
	local hiddenAbility = baseData.HiddenAbility -- String or nil
	-- 1. Ability Assignment (Logic remains the same)
	if hiddenAbility and math.random(1, 100) <= 5 then
		self.ActiveAbility = hiddenAbility
	elseif possibleAbilities and #possibleAbilities > 0 then
		local selectedAbility = possibleAbilities[math.random(1, #possibleAbilities)]
		self.ActiveAbility = selectedAbility
	else
		self.ActiveAbility = nil
	end

	-- Dynamic properties unique to this instance
	self.Level = level
	self.EXP = 0
	self.CurrentStats = CreatureModule:CalculateStats(baseData.BaseStats, self.Level)
	self.CurrentHP = self.CurrentStats.HP

	-- [[ MOVE TACTICAL ASSIGNMENT ]]
	-- 1. Get all moves the creature should know at this level
	local allLearnableMoves = CreatureModule.getMovesLearnedAtLevel(baseData, level)

	-- 2. Assign the 4 most recently learned moves as the default tactical moveset
	local MAX_MOVES = 4
	local currentMoves = {}

	if #allLearnableMoves > 0 then
		-- We need to slice the list to get the last MAX_MOVES entries.
		local start_index = math.max(1, #allLearnableMoves - MAX_MOVES + 1)

		for i = start_index, #allLearnableMoves do
			table.insert(currentMoves, allLearnableMoves[i])
		end
	end

	-- Store the equipped moves. This key is what gets saved/traded!
	self.CurrentMoves = currentMoves 
	-- self.AllKnownMoves = allLearnableMoves -- Optional: To save move history

	return self
end

return CreatureModule]]></ProtectedString>
