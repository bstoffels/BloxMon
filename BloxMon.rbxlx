					<ProtectedString name="Source"><![CDATA[-- CombatModule (In ServerStorage)
-- The central hub for all battle logic, ensuring calculations are server-authoritative.

local CombatModule = {}

-- [[ 1. Module Dependencies ]]
-- The CombatModule requires access to all secure data modules.
local TypeChartModule = require(game.ServerStorage.TypeChartModule)
local CreatureModule = require(game.ServerStorage.CreatureModule)
local MoveModule = require(game.ServerStorage.MoveModule)
local AbilityModule = require(game.ServerStorage.AbilityModule) -- NEW REQUIREMENT

--- Calculates the final damage dealt by an attacker to a defender.
function CombatModule.CalculateDamage(attackerInstance: table, defenderInstance: table, moveName: string): number

	-- Retrieve Secure Move Data
	local moveData = MoveModule.getMoveData(moveName)
	if not moveData then return 0 end -- Safety check

	-- Determine Stats based on Move Category (Physical, Special)
	local attackStatKey = (moveData.Category == "Physical") and "Attack" or "SpAtk"
	local defenseStatKey = (moveData.Category == "Physical") and "Defense" or "SpDef"

	local attackStat = attackerInstance.CurrentStats[attackStatKey]
	local defenseStat = defenderInstance.CurrentStats[defenseStatKey]

	-- Base Formula Calculation (Uses the standard creature formula)
	local damage = (
		( (2 * attackerInstance.Level / 5 + 2) * moveData.Power * (attackStat / defenseStat) / 50 )
	) + 2

	-- Apply Modifiers (The Heart of the Calculation)
	local totalMultiplier = 1

	-- TYPE EFFECTIVENESS
	local typeEffectiveness = 1
	for _, defendingType in ipairs(defenderInstance.Type) do
		local singleTypeMultiplier = TypeChartModule.getMultiplier(moveData.Type, defendingType)
		typeEffectiveness *= singleTypeMultiplier -- Multiply the effects (e.g., 2.0 * 0.5 = 1.0)
	end
	totalMultiplier *= typeEffectiveness

	-- STAB (Same-Type Attack Bonus): 1.5x if the move type matches one of the attacker's types
	for _, type in ipairs(attackerInstance.Type) do
		if type == moveData.Type then
			totalMultiplier *= 1.5
			break
		end
	end
	
	-- ABILITY MODIFIERS: Apply any ability-based modifiers
	local attackerAbilityData = AbilityModule.getAbilityData(attackerInstance.Ability)
	if attackerAbilityData and attackerAbilityData.Trigger == "OnAttackExecuted" then
		local effect = attackerAbilityData.Effect
		if effect[1] == "BoostTypeDamage" and effect[2] == moveData.Type then
			-- Apply the boost (e.g., 1.20x from Pyre Power)
			totalMultiplier *= effect[3]
			print(attackerInstance.Name .. "'s " .. attackerAbilityData.Name .. " boosted damage!")
		end
	end

	-- Critical Hit Logic (e.g., 6.25% chance)
	damage = math.floor(damage * totalMultiplier)

	return math.max(1, damage) -- Damage is at least 1, unless type effectiveness is 0.
end

-- [[ 3. The Server-Authoritative Battle Processor ]]

--- Receives the client's command and processes the battle turn.
-- This function is called by a RemoteEvent listener (which we will set up next).
function CombatModule.ProcessBattleTurn(player, creatureInstanceID: string, moveName: string)
	-- 1. SECURITY CHECK: Authenticate player and ownership.
	-- 2. GET DATA: Retrieve active creature instance data from the ServerDataCache.
	-- 3. EXECUTE: Call CombatModule.CalculateDamage(...)
	-- 4. APPLY STATE: Update defender's HP and check for faints.
	-- 5. NOTIFY CLIENTS: Send the result (damage dealt, type effectiveness) back to the clients for display.
	print(player.Name .. " is attempting to use " .. moveName .. ".")
end

return CombatModule]]></ProtectedString>
